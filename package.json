{
  "name": "@supabase/doctest-js",
  "version": "0.1.1",
  "description": "Run doc examples as tests",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "test": "mocha --require @babel/register",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint {src,test}/**/*.js webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "pretty": "prettier --write \"**/*.js\""
  },
  "author": "Supabase",
  "repository": "https://github.com/supabase/doctest-js",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.1.2",
    "jison": "^0.4.18",
    "lex": "^1.7.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^26.0.0",
    "mocha": "^9.2.2",
    "prettier": "^2.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.0.0"
  }
}
