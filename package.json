{
  "name": "@supabase/doctest-js",
  "version": "0.1.0",
  "description": "Run doc examples as tests",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack -p",
    "test": "mocha --require babel-register",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint {src,test}/**/*.js webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "pretty": "prettier --write \"**/*.js\""
  },
  "author": "Supabase",
  "repository": "https://github.com/supabase/doctest-js",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "jest": "^25.1.0",
    "jison": "^0.4.18",
    "lex": "^1.7.9"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.4",
    "prettier": "^1.19.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.3.0"
  }
}
