{
  "name": "@supabase/doctest-js",
  "version": "0.1.0",
  "description": "Run doc examples as tests",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "test": "mocha --require @babel/register",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint {src,test}/**/*.js webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "pretty": "prettier --write \"**/*.js\""
  },
  "author": "Supabase",
  "repository": "https://github.com/supabase/doctest-js",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.1.2",
    "jest": "^26.0.0",
    "jison": "^0.4.18",
    "lex": "^1.7.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^8.0.1",
    "prettier": "^2.0.1",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0"
  }
}
